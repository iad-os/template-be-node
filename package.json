{
  "name": "template-be-node",
  "description": "Template for a Node.js backend in Typescript",
  "version": "0.0.1",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "tsx --watch ./src/index.ts",
    "build": "tsc",
    "start": "tsx --loader ./src/index.ts",
    "push": "git push origin HEAD && git push origin $(git describe --tags `git rev-list --tags --max-count=1`)",
    "ver": "npm run build && npm version ${npm_config_newversion} --tag-version-prefix=\"\" --preid=${npm_config_preid} --template=\"{{version}}-{{preid}}\""
  },
  "keywords": [],
  "author": "Nicola Vurchio <nicola.vurchio@iad2.it>",
  "license": "ISC",
  "devDependencies": {
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.14.1",
    "@types/qs": "^6.9.18",
    "@types/validator": "^13.15.0",
    "@types/wait-on": "^5.3.4",
    "@typescript-eslint/eslint-plugin": "^8.31.0",
    "@typescript-eslint/parser": "^8.31.0",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-prettier": "^5.2.6",
    "prettier": "3.5.3",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.3",
    "typescript": "^5.8.3"
  },
  "dependencies": {
    "@fastify/autoload": "^6.3.0",
    "@fastify/cors": "^11.0.1",
    "@fastify/helmet": "^13.0.1",
    "@fastify/sensible": "^6.0.3",
    "@fastify/swagger": "^9.5.0",
    "@fastify/swagger-ui": "^5.2.2",
    "@fastify/type-provider-typebox": "^5.1.0",
    "@ghii/ghii-es": "^1.3.1",
    "@ghii/http-loader": "^0.0.8",
    "@ghii/package-json-loader": "^1.1.3",
    "@ghii/yaml-loader": "^0.0.7",
    "@iad-os/ghenghi-es": "^1.0.5",
    "@iad-os/irene-kills-es": "^0.0.7-alpha",
    "@keycloak/keycloak-admin-client": "^26.2.1",
    "@langchain/core": "^0.3.48",
    "@langchain/openai": "^0.5.6",
    "@scalar/fastify-api-reference": "^1.28.26",
